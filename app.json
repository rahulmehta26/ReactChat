{
  "expo": {
    "name": "ReactChat",
    "slug": "ReactChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_white_bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_text.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_text.png",
          "imageWidth": 50,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
